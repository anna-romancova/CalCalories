<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="?android:attr/selectableItemBackground"
    android:clickable="true"
    android:orientation="vertical"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/row_padding_vertical"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/row_padding_vertical">


    <TextView
        android:id="@+id/tvSerchName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/nameTitle"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/tvNameFood"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tvSerchName"
        android:layout_alignParentStart="true"
        android:layout_marginStart="89dp"
        android:layout_marginTop="1dp"
        android:text="text" />


    <TextView
        android:id="@+id/tvSerchProt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvNameFood"
        android:layout_alignStart="@+id/tvSerchName"
        android:text="@string/proteinTitle"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/tvProtein"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvNameFood"
        android:layout_alignStart="@+id/tvNameFood"
        android:text="text" />


    <TextView
        android:id="@+id/tvSerchFats"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tvSerchName"
        android:layout_alignTop="@+id/tvNameFood"
        android:layout_marginStart="4dp"
        android:layout_marginTop="44dp"
        android:text="@string/fatTitle"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/tvFat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tvNameFood"
        android:layout_alignTop="@+id/tvNameFood"
        android:layout_marginStart="0dp"
        android:layout_marginTop="45dp"
        android:text="text" />


    <TextView
        android:id="@+id/tvSerchCarb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvSerchName"
        android:layout_alignStart="@+id/tvSerchCalories"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="3dp"
        android:text="@string/carbohydrateTitle"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/tvCargoh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvSerchName"
        android:layout_alignStart="@+id/tvCalories"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="7dp"
        android:text="text" />

    <TextView
        android:id="@+id/tvSerchCalories"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tvFat"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-1dp"
        android:layout_marginEnd="126dp"
        android:text="@string/caloriesTitle"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/tvCalories"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tvFat"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="35dp"
        android:text="text" />


</RelativeLayout>